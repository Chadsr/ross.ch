{
    "name": "ross.ch",
    "version": "0.1.0",
    "description": "ross.ch website",
    "main": "dist/server.js",
    "author": "Ross Chadwick",
    "sideEffects": false,
    "private": true,
    "scripts": {
        "dev": " NODE_ENV=development nodemon --watch '*' -e ts,tsx --exec ./node_modules/.bin/ts-node src/server.ts",
        "lint": "./node_modules/.bin/eslint . --ext .js,.jsx,.ts,.tsx",
        "lint-fix": "./node_modules/.bin/eslint . --fix --ext .js,.jsx,.ts,.tsx",
        "serve": "node --trace-deprecation dist/src/server.js",
        "webpack": "NODE_ENV=production ./node_modules/.bin/webpack --progress --config ./webpack.config.ts",
        "build": "npm run lint && ./node_modules/.bin/tsc && npm run webpack --display-used-exports && ./node_modules/.bin/ts-node copyStaticAssets.ts",
        "start": "NODE_ENV=production npm run serve"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,ts,tsx}": [
            "eslint --fix"
        ],
        "*.{js,jsx,ts,tsx}": "eslint --cache --fix"
    },
    "engines": {
        "node": "12.x.x",
        "npm": "6.x.x"
    },
    "license": "MIT",
    "homepage": "https://github.com/Chadsr/ross.ch",
    "repository": "github:chadsr/ross.ch",
