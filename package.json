  "description": "ross.ch website",
  "main": "dist/server.js",
  "author": "Ross Chadwick",
  "sideEffects": false,
  "private": true,
  "scripts": {
    "dev": " NODE_ENV=development nodemon --watch '*' -e ts,tsx --exec ./node_modules/.bin/ts-node src/server.ts",
    "lint": "./node_modules/.bin/tslint -c tslint.json -p tsconfig.json",
    "serve": "node --trace-deprecation dist/src/server.js",
    "webpack": "NODE_ENV=production ./node_modules/.bin/webpack --config ./webpack.config.ts",
    "build": "npm run lint && ./node_modules/.bin/tsc && npm run webpack --display-used-exports && ./node_modules/.bin/ts-node copyStaticAssets.ts",
    "start": "NODE_ENV=production npm run serve"
  },
  "engines": {
    "node": "8.x.x",
    "npm": "5.x.x"
  },
