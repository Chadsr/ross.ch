<ul>
    {{#each mediumPosts}}
        <li>
            <a href="{{url}}" target="_blank">
                <h3>{{title}}</h3>
                <span>{{publishDate}}</span>
                <div>
                    {{#each tags}}
                        <span>{{this}}</span>
                    {{/each}}
                </div>
                <p>{{contentSnippet}}</p>
            </a>
        </li>
    {{/each}}
</ul>